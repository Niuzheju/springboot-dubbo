<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd" default-lazy-init="true">

    <dubbo:registry protocol="zookeeper" address="${dubbo.registry.address}" timeout="3000"/>

    <!--分组聚合-->
    <dubbo:reference id="groupMergedService" interface="com.niuzj.user.IGroupMergedService" check="false" group="old,new" merger="true">
       <!-- <dubbo:method name="groupArray" merger="true"/>
        <dubbo:method name="groupList" merger="true"/>
        <dubbo:method name="groupMap" merger="true"/>-->
    </dubbo:reference>
</beans>
